    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.0.3",
    "pdfkit": "^0.14.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=14.0.0"
  }
